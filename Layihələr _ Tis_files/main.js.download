$(document).ready(function () {
  $(".search input").click(function () {
    $(".search form").addClass("active");
    $(".searchClose").addClass("active");
    $(".searchMenuMobile").addClass("active");
  });
  $(".searchClose").click(function () {
    $(".search form").removeClass("active");
    $(this).removeClass("active");
    $(".search form").val();
  });
  $("#get-url-btn").click(function () {
    const url = window.location.href;
    Copy();
    $(this).addClass("active");
  });
  function Copy() {
    var Url = document.getElementById("url");
    Url.innerHTML = window.location.href;
    console.log(Url.innerHTML);
    Url.select();
    document.execCommand("copy");
  }
  $(document).click(function (e) {
    e.stopPropagation();
    var container = $("#get-url-btn");
    if (container.has(e.target).length === 0) {
      $("#get-url-btn").removeClass("active");
    }
  });
  $(".menuOpen").click(function () {
    $(".headerLeft").addClass("active");
  });
  $(".menuCLose").click(function () {
    $(".headerLeft").removeClass("active");
    $(".searchMenuMobile").removeClass("active");
    $(".search form").removeClass("active");
    $(".search input").val();
    console.log("red");
  });
  $(".searchMenuButton").click(function () {
    $(".searchMenuMobile").addClass("active");
    $(".search").addClass("active");
  });



});
